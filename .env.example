# Neptune Configuration Example

# Neptune Web App Name (used in vite.config.js base path)
VITE_NEPTUNE_WEBAPP_NAME=smoketest

# Neptune Server Name
VITE_NEPTUNE_SERVER_URL=https://localhost:8081

# Neptune API Base Path
VITE_NEPTUNE_API_BASE=/neptune/api

# SAP Client
VITE_SAP_CLIENT=800

# Proxy Configuration
VITE_USE_PROXY=true

# Proxy Base Path
VITE_PROXY_BASE=/proxy